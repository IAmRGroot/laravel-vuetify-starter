{
    "name": "iamrgroot/laravel-vuetify-starter",
    "type": "project",
    "description": "Boilerplate for Laravel 8 + Vue 3 + Vuetify.",
    "keywords": ["framework", "laravel", "Vue", "Vuetify"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-pdo_mysql": "*",
        "ext-zip": "*",
        "appstract/laravel-opcache": "^4.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "laravel/framework": "^8.69",
        "laravel/sanctum": "^2.12",
        "laravel/tinker": "^2.6",
        "laravel/ui": "^3.3",
        "sentry/sentry-laravel": "^2.9",
        "spatie/laravel-data": "^1.0",
        "spatie/laravel-permission": "^5.3",
        "spatie/laravel-typescript-transformer": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "brianium/paratest": "^6.3",
        "facade/ignition": "^2.16",
        "fakerphp/faker": "^1.16",
        "friendsofphp/php-cs-fixer": "^3.2",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "larastan": [
            "vendor/bin/phpstan analyse"
        ],
        "format-php": [
            "php ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --verbose --config=./php-cs.php --path-mode=intersection . --ansi"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models -W -r",
            "composer format"
        ],
        "test": [
            "php artisan config:clear",
            "php artisan test",
            "php artisan config:cache"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
